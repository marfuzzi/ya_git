{
  "name": "ya-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./bin/www",
    "build": "npm run lint && gulp build",
    "dev": "export NODE_ENV=development&& nodemon ./bin/www",
    "lint": "eslint **/*.js",
    "csslint": "csscomb -v src/scss/*.scss",
    "babel": "babel",
    "test": "mocha \"**/*.test.js\"",
    "clean": "rimraf node_modules",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "css-mqpacker": "^6.0.2",
    "csscomb": "^4.2.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-yandex": "^1.0.6",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-deporder": "^1.1.0",
    "gulp-htmlclean": "^2.7.20",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^7.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-strip-debug": "^3.0.0",
    "gulp-uglify": "^3.0.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.2",
    "postcss-assets": "^5.0.0",
    "rimraf": "2.5.2"
  }
}
