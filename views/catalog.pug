extends layout

block content
    table(class='table')
        tr
            th= 'Files'
            tbody
                each val in fileList
                    tr
                    if (val.type === 'blob')
                        td
                            a(href=`/file/${val.hash}`)= val.name
                    else
                        td
                            div(class='folder')
                            a(href=`/catalog/${val.hash}`)= val.name
